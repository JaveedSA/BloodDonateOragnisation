<%- include("partials/header"); -%>


<div class="MarginTop">
  <% if(posts!=null) { %>
  <table class="table table-striped table-border">
    <tbody>
      <tr class="table-success">
        <th colspan="6" style="color:#3d1818; padding-left:35%; word-spacing: 20px;">Availability Of Blood Donors</th>
      </tr>

      <tr>
        <th><em>First_Name</em></th>
        <th><em>Last Name</em> </th>
        <th><em>Numbers</em></th>
        <th><em>State</em> </th>
        <th><em>District</em></th>
        <th><em>BloodGroup</em></th>
      </tr>
      <% posts.forEach(function(item){ %>
      <tr>
        <td><%= item.form1.firstname%></td>
        <td><%= item.form1.lastname %></td>
        <td><%= item.form1.numbers%></td>
        <td><%= item.form1.statename %></td>
        <td><%= item.form1.districtname %></td>
        <td><%= item.form1.bloodgr %></td>

      </tr>
      <% }) %>
    </tbody>
  </table>
  <% } %>


  <section class="footerhome1">
    <!-- footer-bottom -->
    <div class="copyright">Copyright Â© 2021 <a href="#">Blood Donate Organization India</a>. All Rights Reserved.</div>
  </section>
  <div class="scroll-to-top scroll-to-target" data-target=".main-header"><span class="icon fa fa-angle-up"></span>
  </div>
  <script src="index.js" charset="utf-8"></script>
